set $mode_mail Email   Click   Enter   +Enter   Prev   Next   PageUp   PageDown   Grab-On   Space   Esc
bindsym $mod+Shift+m exec --no-startup-id python ~/.i3/python ~/.i3/save_mail.py, mode "$mode_mail"
mode "$mode_mail" {
	bindsym 1 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 0 0
    bindsym 2 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 0 1
    bindsym 3 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 0 2
    bindsym 4 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 0 3
    bindsym 5 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 0 4
    bindsym 6 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 0 5
    bindsym 7 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 0 6
    bindsym 8 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 0 7
    bindsym 9 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 0 8
    bindsym 0 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 0 9

    bindsym $mod+1 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 1 0
    bindsym $mod+2 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 1 1
    bindsym $mod+3 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 1 2
    bindsym $mod+4 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 1 3
    bindsym $mod+5 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 1 4
    bindsym $mod+6 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 1 5
    bindsym $mod+7 exec --no-startup-id killall dunst send_mail, exec --no-startup-id ~/.i3/send_mail 1 6
    bindsym $mod+8 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 1 7
    bindsym $mod+9 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 1 8
    bindsym $mod+0 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 1 9

    bindsym $mod+Shift+1 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 2 0
    bindsym $mod+Shift+2 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 2 1
    bindsym $mod+Shift+3 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 2 2
    bindsym $mod+Shift+4 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 2 3
    bindsym $mod+Shift+5 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 2 4
    bindsym $mod+Shift+6 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 2 5
    bindsym $mod+Shift+7 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 2 6
    bindsym $mod+Shift+8 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 2 7
    bindsym $mod+Shift+9 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 2 8
    bindsym $mod+Shift+0 exec --no-startup-id killall send_mail, exec --no-startup-id ~/.i3/send_mail 2 9

    bindsym Left exec --no-startup-id py3-cmd click --button 9 group
    bindsym Right exec --no-startup-id py3-cmd click --button 8 group
    bindsym Up exec --no-startup-id dunstctl close
    bindsym Down exec --no-startup-id py3-cmd click --button 1 imap

    bindsym space exec --no-startup-id killall send_mail, exec --no-startup-id killall dunst
    bindsym Escape exec --no-startup-id killall send_mail, exec --no-startup-id killall dunst, mode "default"
}

set $mode_ydcv_on ydcv   Click   Enter   +Enter   Prev   Next   PageUp   PageDown   Grab-On   Space   Esc
bindsym $mod+Shift+s exec --no-startup-id ~/.i3/send_ydcv_screen, exec --no-startup-id dunstify -u normal "  Screen grab words...", mode "$mode_ydcv_on"
mode "$mode_ydcv_on" {
    bindsym Return exec --no-startup-id dunstify -u critical "$(ydcv $(xclip -o))"
    bindsym Shift+Return exec --no-startup-id python ~/.i3/get_jukuu.py 
    bindsym space exec --no-startup-id killall dunst
    bindsym Escape exec --no-startup-id killall dunst, exec --no-startup-id killall send_ydcv_screen, mode "default"

    bindsym m exec --no-startup-id dunstify "$(ydcv $(xclip -o))"
    bindsym comma exec --no-startup-id dunstify "$(ydcv $(xclip -o | tr  -d ' \n \t'))"
    bindsym period exec --no-startup-id python ~/.i3/get_jukuu.py 
    bindsym Ctrl+period exec --no-startup-id python ~/.i3/get_jukuu.py 
    bindsym 1 exec --no-startup-id python ~/.i3/get_jukuu.py  1
    bindsym 2 exec --no-startup-id python ~/.i3/get_jukuu.py  2
