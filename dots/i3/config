set $mode_fortune fortune-mod:  Deutch  English  Fan:)  Chinese  Tang-Song  Japanese  Wisdom  Science  Bible  Quran
bindsym $mod+bracketleft exec --no-startup-id killall dunst, mode "$mode_fortune"
mode "$mode_fortune" {
    bindsym r exec --no-startup-id dunstify -u normal "$(cal)" "$(date --rfc-3339=s)"
    bindsym $mod+r exec --no-startup-id dunstify -u normal "$(cal)" "$(date --rfc-3339=ns)"
    bindsym w exec --no-startup-id dunstify -u normal "$(sensors)"
    bindsym $mod+w exec --no-startup-id dunstify -u normal "$(sensors)"
    bindsym p exec --no-startup-id dunstify -u normal "$(ping -c 1 google.com)"
    bindsym $mod+p exec --no-startup-id dunstify -u normal "$(ping -c 1 google.com)"
    bindsym u exec --no-startup-id dunstify -u normal "$(mpstat)"
    bindsym $mod+u exec --no-startup-id dunstify -u normal "$(mpstat)"
    bindsym $mod+u exec --no-startup-id dunstify -u normal "$(mpstat)"

    bindsym d exec --no-startup-id dunstify "$(fortune Vokabular German)"
    bindsym e exec --no-startup-id dunstify "$(fortune gre subset)"
    bindsym f exec --no-startup-id dunstify "$(fortune jokes)"
    bindsym c exec --no-startup-id dunstify "$(fortune chengyu chinese diet)"
    bindsym j exec --no-startup-id dunstify "$(fortune dict_ch-jp dict_jp-ch jp_gy jp_sy | tr -d '\n')"
    bindsym t exec --no-startup-id dunstify "$(fortune tang300 song100)"
    bindsym w exec --no-startup-id dunstify "$(fortune wisdom love art paradoxum)"
    bindsym s exec --no-startup-id dunstify "$(fortune definitions science education issa-haiku | tr -d '\n')"
    bindsym b exec --no-startup-id dunstify "$(fortune bible bible_en)"
    bindsym q exec --no-startup-id dunstify "$(fortune quran)"
    bindsym a exec --no-startup-id dunstify "$(fortune | tr -d '\n')"

    bindsym $mod+Shift+d exec --no-startup-id dunstify -u critical "$(fortune Vokabular German)"
    bindsym $mod+Shift+e exec --no-startup-id dunstify -u critical "$(fortune gre subset)"
    bindsym $mod+Shift+f exec --no-startup-id dunstify -u critical "$(fortune jokes)"
    bindsym $mod+Shift+c exec --no-startup-id dunstify -u critical "$(fortune chengyu chinese diet)"
    bindsym $mod+Shift+j exec --no-startup-id dunstify -u critical "$(fortune dict_ch-jp dict_jp-ch jp_gy jp_sy | tr -d '\n')"
    bindsym $mod+Shift+t exec --no-startup-id dunstify -u critical "$(fortune tang300 song100)"
    bindsym $mod+Shift+w exec --no-startup-id dunstify -u critical "$(fortune wisdom love art paradoxum)"
    bindsym $mod+Shift+s exec --no-startup-id dunstify -u critical "$(fortune definitions science education issa-haiku | tr -d '\n')"
    bindsym $mod+Shift+b exec --no-startup-id dunstify -u critical "$(fortune bible bible_en)"
    bindsym $mod+Shift+q exec --no-startup-id dunstify -u critical "$(fortune quran)"
    bindsym $mod+Shift+a exec --no-startup-id dunstify -u critical "$(fortune | tr -d '\n')"

    bindsym $mod+d exec --no-startup-id dunstify -u low "$(fortune Vokabular German)"
    bindsym $mod+e exec --no-startup-id dunstify -u low "$(fortune gre subset)"
    bindsym $mod+f exec --no-startup-id dunstify -u low "$(fortune jokes)"
    bindsym $mod+c exec --no-startup-id dunstify -u low "$(fortune chengyu chinese diet)"
    bindsym $mod+j exec --no-startup-id dunstify -u low "$(fortune dict_ch-jp dict_jp-ch jp_gy jp_sy | tr -d '\n')"
    bindsym $mod+t exec --no-startup-id dunstify -u low "$(fortune tang300 song100)"
    bindsym $mod+w exec --no-startup-id dunstify -u low "$(fortune wisdom love art paradoxum)"
    bindsym $mod+s exec --no-startup-id dunstify -u low "$(fortune definitions science education issa-haiku | tr -d '\n')"
    bindsym $mod+b exec --no-startup-id dunstify -u low "$(fortune bible bible_en)"
    bindsym $mod+q exec --no-startup-id dunstify -u low "$(fortune quran)"
    bindsym $mod+a exec --no-startup-id dunstify -u low "$(fortune | tr -d '\n')"

    bindsym space exec --no-startup-id killall dunst
    bindsym Escape exec --no-startup-id killall dunst, mode "default"
}

set $mode_mouse δ-mode
bindsym $mod+m exec --no-startup-id killall dunst, mode "$mode_mouse"
bindsym $mod+period mode "$mode_mouse"
bindsym $mod+comma mode "$mode_mouse"
bindsym $mod+Zenkaku_Hankaku exec --no-startup-id xdotool click 2
bindsym $mod+slash exec --no-startup-id xdotool click 1
bindsym $mod+backslash exec --no-startup-id xdotool click 3
bindsym XF86Launch7 exec --no-startup-id xdotool click 1
bindsym XF86Launch8 exec --no-startup-id xdotool click 2
