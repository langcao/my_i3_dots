    bindsym 8 exec --no-startup-id python ~/.i3/get_jukuu.py  8
    bindsym 9 exec --no-startup-id python ~/.i3/get_jukuu.py  9
    bindsym 0 exec --no-startup-id python ~/.i3/get_jukuu.py  0
    bindsym backslash exec --no-startup-id python ~/.i3/get_jukuu.py  PageUp
    bindsym slash exec --no-startup-id python ~/.i3/get_jukuu.py  PageDown
    bindsym Left exec --no-startup-id python ~/.i3/get_jukuu.py  PageUp
    bindsym Right exec --no-startup-id python ~/.i3/get_jukuu.py  PageDown
    bindsym bracketleft exec --no-startup-id python ~/.i3/get_jukuu.py  ''
    bindsym bracketright exec --no-startup-id python ~/.i3/get_jukuu.py  ''
    bindsym Up exec --no-startup-id python ~/.i3/get_jukuu.py  ''
    bindsym Down exec --no-startup-id python ~/.i3/get_jukuu.py  ''
    bindsym a exec --no-startup-id python ~/.i3/get_jukuu.py  minute
    bindsym s exec --no-startup-id python ~/.i3/get_jukuu.py  hour
    bindsym d exec --no-startup-id python ~/.i3/get_jukuu.py  day
    bindsym z exec --no-startup-id python ~/.i3/get_jukuu.py  week
    bindsym x exec --no-startup-id python ~/.i3/get_jukuu.py  month
    bindsym c exec --no-startup-id python ~/.i3/get_jukuu.py  year
    bindsym f exec --no-startup-id python ~/.i3/get_jukuu.py  sorted
    bindsym v exec --no-startup-id python ~/.i3/get_jukuu.py  inversed
    bindsym g exec --no-startup-id python ~/.i3/get_jukuu.py  user

    bindsym $mod+m exec --no-startup-id dunstify -u critical "$(ydcv $(xclip -o))"
    bindsym $mod+comma exec --no-startup-id dunstify -u critical "$(ydcv $(xclip -o  | tr  -d ' \n \t'))"
    bindsym $mod+period exec --no-startup-id python ~/.i3/get_jukuu.py  0 critical
    bindsym Ctrl+$mod+period exec --no-startup-id python ~/.i3/get_jukuu.py  0 critical
    bindsym $mod+1 exec --no-startup-id python ~/.i3/get_jukuu.py  1 critical
    bindsym $mod+2 exec --no-startup-id python ~/.i3/get_jukuu.py  2 critical
    bindsym $mod+3 exec --no-startup-id python ~/.i3/get_jukuu.py  3 critical
    bindsym $mod+4 exec --no-startup-id python ~/.i3/get_jukuu.py  4 critical
    bindsym $mod+5 exec --no-startup-id python ~/.i3/get_jukuu.py  5 critical
    bindsym $mod+6 exec --no-startup-id python ~/.i3/get_jukuu.py  6 critical
    bindsym $mod+7 exec --no-startup-id python ~/.i3/get_jukuu.py  7 critical
    bindsym $mod+8 exec --no-startup-id python ~/.i3/get_jukuu.py  8 critical
    bindsym $mod+9 exec --no-startup-id python ~/.i3/get_jukuu.py  9 critical
    bindsym $mod+0 exec --no-startup-id python ~/.i3/get_jukuu.py  0 critical
    bindsym $mod+backslash exec --no-startup-id python ~/.i3/get_jukuu.py  PageUp critical
    bindsym $mod+slash exec --no-startup-id python ~/.i3/get_jukuu.py  PageDown critical
    bindsym $mod+Left exec --no-startup-id python ~/.i3/get_jukuu.py  PageUp critical
    bindsym $mod+Right exec --no-startup-id python ~/.i3/get_jukuu.py  PageDown critical
    bindsym $mod+bracketleft exec --no-startup-id python ~/.i3/get_jukuu.py  '' critical
    bindsym $mod+bracketright exec --no-startup-id python ~/.i3/get_jukuu.py  '' critical
    bindsym $mod+Up exec --no-startup-id python ~/.i3/get_jukuu.py  '' critical
    bindsym $mod+Down exec --no-startup-id python ~/.i3/get_jukuu.py  '' critical
    bindsym $mod+a exec --no-startup-id python ~/.i3/get_jukuu.py  minute critical
    bindsym $mod+s exec --no-startup-id python ~/.i3/get_jukuu.py  hour critical
    bindsym $mod+d exec --no-startup-id python ~/.i3/get_jukuu.py  day critical
    bindsym $mod+z exec --no-startup-id python ~/.i3/get_jukuu.py  week critical
    bindsym $mod+x exec --no-startup-id python ~/.i3/get_jukuu.py  month critical
    bindsym $mod+c exec --no-startup-id python ~/.i3/get_jukuu.py  year critical
    bindsym $mod+f exec --no-startup-id python ~/.i3/get_jukuu.py  sorted critical
    bindsym $mod+v exec --no-startup-id python ~/.i3/get_jukuu.py  inversed critical
    bindsym $mod+g exec --no-startup-id python ~/.i3/get_jukuu.py  user critical

    bindsym h exec --no-startup-id google-chrome-stable ~/.cache/jukuu/words.txt, exec --no-startup-id dunstify -u low " Jukuu wordlist is loaded."
    bindsym b exec --no-startup-id google-chrome-stable ~/.cache/jukuu/output.txt, exec --no-startup-id dunstify -u low " Recent output is loaded."
    bindsym $mod+h exec --no-startup-id google-chrome-stable ~/.cache/jukuu/, exec --no-startup-id dunstify -u low " Jukuu .cache is loaded." ""
    bindsym $mod+BackSpace exec --no-startup-id python ~/.i3/get_jukuu.py 
    bindsym Shift $mod+BackSpace exec --no-startup-id python ~/.i3/get_jukuu.py 
    bindsym Ctrl+$mod+BackSpace exec --no-startup-id rm -r ~/.cache/jukuu/dels.txt, exec --no-startup-id dunstify -u low " Deteleted wordlist is cleared." ""
    bindsym Ctrl+Shift+$mod+BackSpace exec --no-startup-id rm -r ~/.cache/jukuu/, exec --no-startup-id dunstify -u low " Jukuu .cache directory is cleared." ""
}


