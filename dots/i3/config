    bindsym $mod+a workspace $ws_home, exec --no-startup-id atom, mode "default"
    bindsym $mod+b workspace $ws_win, exec --no-startup-id virtualbox, mode "default"
    bindsym $mod+c exec --no-startup-id google-chrome-stable, mode "default"
