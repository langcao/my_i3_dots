set $mode_launcher Atom  virtualBox  Chrome  Dropbox  Emailbox  Firefox  Geany  Lazarus  Matlab  Pycharm  Qtcreator  R  Sublime  Tilda  nVim  WeChat  lateX  Youtube
bindsym $mod+o mode "$mode_launcher"

mode "$mode_launcher" {
    bindsym a workspace $ws_home, exec --no-startup-id atom, mode "default"
    bindsym b workspace $ws_win, exec --no-startup-id virtualbox, mode "default"
    bindsym c exec --no-startup-id google-chrome-stable, mode "default"
    #bindsym c workspace $ws_joy, exec --no-startup-id cgoban3, mode "default"
    bindsym d exec --no-startup-id dropbox, mode "default"
    bindsym e workspace $ws_link, exec --no-startup-id sylpheed, mode "default"
    bindsym f workspace $ws_brower, exec --no-startup-id firefox -n, mode "default"
    bindsym g workspace $ws_c, exec --no-startup-id geany, mode "default"
    #bindsym i exec --no-startup-id feh --bg-fill ~/.config/i3/.wallpaper.jpg, mode "default"
    #bindsym j exec --no-startup-id systemd-run --on-calendar="*-*-* *:*/5:00" --user -- \ /usr/bin/feh --no-fehbg --bg-scale --randomize -r ~/Pictures/Wallpapers, mode "default"
    bindsym j workspace $ws_joy, exec --no-startup-id steam, mode "default"
    bindsym h workspace ,  exec --no-startup-id google-chrome-stable --incognito --new-window https://jp.pornhub.com/, mode "default"
    bindsym l workspace $ws_z, exec --no-startup-id lazarus --class floatingApp, mode "default"
    bindsym m workspace $ws_n, exec --no-startup-id lilyterm -E matlab, mode "default"
    bindsym n exec nautilus, mode "default"
    bindsym o exec --no-startup-id killall cmus, exec --no-startup-id guake -e cmus --show, mode "default"
    bindsym p workspace $ws_p, exec --no-startup-id pycharm-edu, mode "default"
    bindsym q workspace $ws_q, exec --no-startup-id qtcreator, mode "default"
    bindsym r workspace $ws_r, exec --no-startup-id rstudio-bin, mode "default"
    bindsym s exec subl3, mode "default"
    #bindsym t exec --no-startup-id tilda -c ydcv, mode "default"
    #bindsym t exec --no-startup-id stardict --class floatingApp, mode "default"
    #bindsym u exec --no-startup-id nautilus /run/media/fandy && umount /dev/sdb1, mode "default"
    bindsym v exec --no-startup-id guake -e nvim, mode "default"
    bindsym w workspace $ws_wechat, exec --no-startup-id electronic-wechat  --class floatingApp, mode "default"
